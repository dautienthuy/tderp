<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="seq_maintenance_request" model="ir.sequence">
        <field name="name">Maintenance Request</field>
        <field name="code">maintenance.request</field>
        <field name="prefix">MR</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="seq_maintenance_equipment" model="ir.sequence">
        <field name="name">Maintenance Equipment</field>
        <field name="code">maintenance.equipment</field>
        <field name="prefix">ME</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="seq_sale_contract" model="ir.sequence">
        <field name="name">Sale Contract</field>
        <field name="code">sale.contract</field>
        <field name="prefix">SC</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Bảo trì: BGBTxxxxx -->
    <record id="seq_sale_order_bt" model="ir.sequence">
        <field name="name">SO Bảo trì</field>
        <field name="code">sale.order.bt</field>
        <field name="prefix">BGBT</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Bán mới: BGBMxxxxx -->
    <record id="seq_sale_order_bm" model="ir.sequence">
        <field name="name">SO Bán mới</field>
        <field name="code">sale.order.bm</field>
        <field name="prefix">BGBM</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Sửa chữa: xxxmonth.y(s)/BGSC -->
    <record id="seq_sale_order_sc" model="ir.sequence">
        <field name="name">SO Sửa chữa</field>
        <field name="code">sale.order.sc</field>
        <field name="prefix"></field>
        <field name="suffix">%(month)s.%(y)s/BGSC</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>
