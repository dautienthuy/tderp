<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="td_hidden_module_group" model="res.groups">
            <field name="name">Hidden</field>
        </record>

        <record id="td_gs_module_group" model="res.groups">
            <field name="name">GS</field>
            <field name="comment">Using code</field>
        </record>

        <record id="td_ktv_module_group" model="res.groups">
            <field name="name">KTV</field>
            <field name="comment">Using code</field>
        </record>

        <record id="td_sale_module_group" model="res.groups">
            <field name="name">Sale</field>
            <field name="comment">Using code</field>
        </record>

        <record id="td_gsktv_module_group" model="res.groups">
            <field name="name">GSKTV</field>
            <field name="comment">Using code</field>
            <field name="implied_ids" eval="[
                  ref('td_module.td_ktv_module_group')
                ]"/>
        </record>

        <record id="td_maintain_module_group" model="res.groups">
            <field name="name">Maintain</field>
            <field name="comment">Using code</field>
        </record>

        <record id="td_admin_module_group" model="res.groups">
            <field name="name">Admin</field>
            <field name="comment">Using code</field>
            <field name="implied_ids" eval="[
                  ref('td_module.td_gs_module_group'),
                  ref('td_module.td_gsktv_module_group'),
                  ref('td_module.td_maintain_module_group'),
                  ref('td_module.td_sale_module_group')
                ]"/>
        </record>

        <record id="td_admin_all_module_group" model="res.groups">
            <field name="name">Admin all</field>
            <field name="comment">Using code</field>
            <field name="implied_ids" eval="[
                  ref('td_module.td_maintain_module_group'),
                  ref('td_module.td_sale_module_group')
                ]"/>
        </record>
    </data>
</odoo>
